  Port 20298
  User ubuntu
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa2
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null

Host air-bcm-01
  HostName bcm-01
  User ubuntu
  ProxyJump air-oob
  PreferredAuthentications publickey
  IdentityFile ~/.ssh/id_rsa2
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
